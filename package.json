{
  "name": "file-browser",
  "description": "File browser app/server application for browsing contents of system attached storage devices.",
  "version": "1.0.0",
  "author": "Rainner Lins <rainnerlins@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --watch --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "user": "node ./server/user.js",
    "node": "node ./server/server.js",
    "nodemon": "nodemon ./server/server.js",
    "pm2": "pm2 start ./server/server.js"
  },
  "dependencies": {
