    "hapi-auth-cookie": "^7.0.0",
    "inert": "^4.2.1",
    "leveldown": "^2.1.1",
    "levelup": "^2.0.2",
    "md5": "^2.2.1",
    "mime": "^2.3.1",
    "sharp": "github:lovell/sharp",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^5.0.1",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.11.2"
  }
